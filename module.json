{
	"comment": "Module definition file.",
	"name": "file-upload",
	"description": "Upload files as attachments for sObjects.",
	"connectedApp": "default",
	"files": [],
	"links":[
		{
        "href":"/file/upload",
        "label": "Upload Files",
        "isSublink": false,
        "adminLink": false
		},
		{
		"href":"/file/list",
		"label": "My Shared Files",
		"isSublink": false,
		"adminLink": false
		}
	],
	"routes": {
		"file/upload": {
			"callback":"showForm",
			"content-type":"text/html"
		},
		"file/upload/file": {
			"callback":"upload",
			"content-type":"text/html"
		},
		"file/list": {
			"callback":"showAll",
			"content-type":"text/html"
		},
		"file/download/%id": {
			"callback":"downloadContentDocument",
			"content-type":"text/html"
		}

	}
}